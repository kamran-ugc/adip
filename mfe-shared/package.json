{
  "name": "shared-components",
  "version": "0.0.3",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "clean:install": "rm -rf node_modules && rm -rf package-lock.json && rm -rf projects/shared-components/node_modules && npm install",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "copy-styles": "node copy-styles.js",
    "storybook": "npm run copy-styles && node projects/shared-components/.storybook/compile-scss.js && ng run shared-components:storybook",
    "build-storybook": "npm run copy-styles && ng run shared-components:build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular-architects/module-federation": "^18.0.6",
